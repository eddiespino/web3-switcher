{
    "manifest_version": 3,
    "name": "Web3Switcher",
    "version": "1.0",
    "description": "Change the Hive  to your liking",
    "permissions": [
      "tabs"
    ],
    "icons": {
      "150": "images/Web3Switcher.png",
    "action": {
      "default_icon": {
        "150": "Web3Switcher.png"
    },
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "*://hive.blog/*",
      "*://peakd.com/*",
      "*://ecency.com/*",
      "*://inleo.io/*"
    ]
  }  
  
