{
    "manifest_version": 3,
    "name": "Web3Switcher",
    "version": "1.1",
    "description": "Easily switch between different Hive frontends to enhance your browsing experience.",
    "permissions": [
      "tabs"
    ],
    "icons": {
      "128": "images/Web3Switcher.png"
    },
    "action": {
      "default_icon": {
        "128": "Web3Switcher.png"
    },
      "default_popup": "popup.html"
    },
    "host_permissions": [
      "*://hive.blog/*",
      "*://peakd.com/*",
      "*://ecency.com/*",
      "*://inleo.io/*"
    ]
}  
  
